pipeline {
    agent any
    stages {
        stage('Verify') {
            steps {
                sh '''
                  dotnet --list-sdks
                  dotnet --list-runtimes
                '''
                sh 'printenv'
                sh 'ls -l "$WORKSPACE"'
            }
        }
        stage('Unit Test') {
            steps {
                sh 'dotnet test "$WORKSPACE/Britannica.UnitTest/Britannica.UnitTest.csproj"'
            }
        }
    }
}
